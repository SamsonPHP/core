    "php",
    "framework",
    "samsonos",
    "samsonphp"
  ],
  "authors": [
    {
      "name": "Vitaly Iegorov",
      "email": "egorov@samsonos.com"
    }
  ],
  "license": "Open Software License (OSL) v 3.0",
  "homepage": "http://samsonphp.com/",
